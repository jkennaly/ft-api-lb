{
	"initial:before": {
		"loopback#favicon": {
			"params": "$!../static/favicon.ico"
		}
	},
	"initial": {
		"compression": {},
		"cors": {
			"params": {
				"origin": true,
				"credentials": true,
				"maxAge": 86400
			}
		},
		"helmet-csp": {
			"params": {
				"directives": {
					"connectSrc": [
						"'self'",
						"https://api.stripe.com",
						"https://checkout.stripe.com"
					],
					"frameSrc": [
						"'self'",
						"https://js.stripe.com",
						"https://hooks.stripe.com",
						"https://checkout.stripe.com"
					],
					"defaultSrc": [
						"'self'"
					],
					"styleSrc": [
						"'self'",
						"fonts.googleapis.com",
						"use.fontawesome.com",
						"'unsafe-inline'"
					],
					"scriptSrc": [
						"'self'",
						"js.stripe.com",
						"https://checkout.stripe.com",
						"https://www.googletagmanager.com",
						"https://widget.cloudinary.com/",
						"https://upload-widget.cloudinary.com/",
						"https://use.fontawesome.com/",
						"'unsafe-inline'"
					],
					"fontSrc": [
						"'self'",
						"fonts.googleapis.com",
						"fonts.gstatic.com",
						"use.fontawesome.com"
					],
					"imgSrc": [
						"'self'",
						"https://*.stripe.com"
					]
				}
			}
		},
		"helmet#xssFilter": {},
		"helmet#frameguard": {
			"params": {
				"action": "deny"
			}
		},
		"helmet#hsts": {
			"params": {
				"maxAge": 0,
				"includeSubDomains": true
			}
		},
		"helmet#hidePoweredBy": {},
		"helmet#ieNoOpen": {},
		"helmet#noSniff": {},
		"helmet#noCache": {
			"enabled": false
		}
	},
	"session": {},
	"auth:before": {
		"./middleware/get-db-conn": {},
		"./middleware/auth-check": {}
	},
	"auth": {
		"./middleware/get-user-id": {
			"paths": "/api/Profiles/getUserId*"
		},
		"./middleware/verify-user-id": {
			"paths": "/api/*"
		}
	},
	"auth:after": {
		"./middleware/gt-access-token": {}
	},
	"parse": {},
	"routes:before": {},
	"routes:after": {
		"loopback#rest": {
			"paths": [
				"${restApiRoot}"
			]
		}
	},
	"files": {
		"loopback#static": [
			{
				"params": "$!./views/public"
			},
			{
				"params": "$!../static"
			}
		]
	},
	"final": {
		"loopback#urlNotFound": {}
	},
	"final:after": {
		"./middleware/stripe-errors": {},
		"./middleware/handle-errors": {},
		"strong-error-handler": {}
	}
}